#include<iostream>
using namespace std;


int main(){
	int num;
	cin >> num;

	for (int i = 0; i < num; i++) {
		for (int j = num - 1; j > i; j--) {
			cout << "a";
		}
		
		cout << "*";
	

		for (int k = 0; k < 2 * i - 1; k++) {
			cout << "a";
		}
		cout << "\n";
		if (i >= 1) {
			
		}
		cout << "\n";

	
	}
