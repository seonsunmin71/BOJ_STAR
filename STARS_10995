#include<iostream>
using namespace std;

char arr[397][397];


int main() {
	int num; // 1 <= num <= 100
	cin >> num;

	for (int i = 0; i < num; i++) {
		if (i % 2 == 0) {
			cout << "*";

			for (int k = 0; k < num - 1; k++) {
				cout << " *";
			}

		}
		else
			for (int j = 0; j < num; j++) {
				cout << " *";
			}

		cout << "\n";
	}


	return 0;
}
